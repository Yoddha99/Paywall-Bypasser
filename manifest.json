{
    "name": "Paywall-Bypasser",
    "description": "Information wants to be free - Aaron Swartz",
    "manifest_version": 2,
    "version": "1.1",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "cookies",
        "webRequest",
        "storage",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "icons": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
    }
}